{
    "name": "lerouse/laravel-repository",
    "description": "Repository implementation for Laravel.",
    "keywords": ["laravel", "eloquent", "database", "repository"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Innerbichler",
            "email": "christian.innerbichler@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "illuminate/contracts": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/events": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lerouse\\LaravelRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lerouse\\LaravelRepository\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lerouse\\LaravelRepository\\LaravelRepositoryServiceProvider"
            ]
        }
    }
}
